<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
         xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" 
         version="3.1">
  <display-name>TylerRaney</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
        <servlet-name>tylerraney</servlet-name>
        <servlet-class>
            org.springframework.web.servlet.DispatcherServlet
        </servlet-class>
        <init-param>
			<param-name>contextConfigLocation</param-name>
			<param-value>/WEB-INF/tylerraney-servlet.xml</param-value>
		</init-param>
        <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
        <servlet-name>tylerraney</servlet-name>
        <url-pattern>/</url-pattern>
      <!--  <url-pattern>/tylerRaneyProject.jsp</url-pattern>
        <url-pattern>/tylerRaneyProject.html</url-pattern>
        <url-pattern>/vcciccProject.jsp</url-pattern>
        <url-pattern>/vcciccProject.html</url-pattern>
        <url-pattern>/seniorProject.jsp</url-pattern>
        <url-pattern>/senioryProject.html</url-pattern>
        <url-pattern>*.html</url-pattern> -->
  </servlet-mapping> 
</web-app>